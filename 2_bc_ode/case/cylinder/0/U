FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    down
    {
        type            fixedValue;
        value    	uniform (0 0 0);
    }

    right
    {
        type            zeroGradient;
    }

    up
    {
        type            fixedValue;
        value    	uniform (0 0 0);
    }

    left
    {
        type            flowRateInletVelocity;
	meanVelocity	1;
	//profile 	sine;
	//amplitude	1;
	//frequency	10;
	//level		0;
    }

    cylinder
    {
        type            fixedValue;
        value    	uniform (0 0 0);
    }

    defaultFaces
    {
        type            empty;
    }
}

